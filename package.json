{
  "name": "naive-admin",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js,.vue src",
    "lint:style": "stylelint **/*.{css,scss,html,vue}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vicons/carbon": "^0.11.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.24.4",
    "vfonts": "^0.0.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@mario34/eslint-config-vue3": "^0.3.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "eslint": "^8.6.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "sass": "^1.47.0",
    "stylelint": "^14.5.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint --fix",
    "*.{vue,css,scss}": "stylelint **/*.{css,scss,vue,html} --fix"
  }
}
